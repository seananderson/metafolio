  number	= {16},
  pages		= {6720--6725},
  title		= {Projected impacts of climate change on salmon habitat
		  restoration},
  volume	= {104},
  year		= {2007},
  bdsk-file-1	= {}
		  ,
  bdsk-url-1	= {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17412830&retmode=ref&cmd=prlinks}
		  
